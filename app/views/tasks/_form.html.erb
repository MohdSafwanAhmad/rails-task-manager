<!-- app/views/restaurants/new.html.erb -->

<%= form_with model: @task do |f| %>
  <div class="container mt-4 p-4">
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :details %>
    <%= f.text_field :details %>
    <%= f.label :completed %>
    <%= f.check_box :completed %>
    <%= f.submit %>
  </div>
<% end %>